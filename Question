#include<bits/stdc++.h>
using namespace std;


    int main()
    {
        string s;
        cin>>s;
        bool flag=false;
        int ans=0;
        string res="";
        
        for(int i=0; i<s.length(); i++)
        {
            if(isdigit(s[i]))
            {
                flag=true;
                int temp=0;
                temp = stoi(s[i]);
                i++;
                
                while(flag)
                {
                    if(isdigit(s[i]))
                    {
                        flag =false;
                        break;
                    }
                    
                    else
                    {
                        temp= temp*10 + stoi(s[i]);
                        i++;
                    }
                    
                }
                
                ans+=temp;
            }
            
            else
            res+=s[i];
            
        }
        cout<<res<<endl;
        cout<<ans;
    }

